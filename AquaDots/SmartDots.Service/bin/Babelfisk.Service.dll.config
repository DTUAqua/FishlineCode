        <endpoint address="" binding="wsHttpBinding" bindingConfiguration="wsHttp" name="SmartDotsEndPoint" contract="Babelfisk.Service.ISmartDots"/>
        <endpoint address="mex" binding="mexHttpBinding" contract="IMetadataExchange"/>
        <host>
