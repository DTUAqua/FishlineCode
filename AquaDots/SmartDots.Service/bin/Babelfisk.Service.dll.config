          <reliableSession ordered="true" inactivityTimeout="00:10:00"
            enabled="false" />
          <security mode="Message">
            <message clientCredentialType="UserName" negotiateServiceCredential="false"
              establishSecurityContext="false" />
          </security>
        </binding>
